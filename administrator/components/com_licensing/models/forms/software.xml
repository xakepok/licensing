<?xml version="1.0" encoding="utf-8"?>
<form addrulepath="/administrator/components/com_licensing/models/rules">
    <fieldset name="names">
        <field
                name="id"
                type="hidden"
        />
        <field
                name="licenseID"
                type="License"
                label="COM_LICENSING_LICENSES_HEAD_NAME"
                description="COM_LICENSING_LICENSES_HEAD_NAME_DESC"
                required="true"
                class="input-xlarge"
        />
        <field
                name="name"
                type="text"
                label="COM_LICENSING_SOFTWARE_HEAD_NAME"
                description="COM_LICENSING_SOFTWARE_HEAD_NAME_DESC"
                required="true"
                autocomplete="off"
                class="input-xlarge"
        />
        <field
                name="unlim"
                type="list"
                label="COM_LICENSING_SOFTWARE_HEAD_COUNT_UNLIM"
                description="COM_LICENSING_SOFTWARE_HEAD_COUNT_UNLIN_DESC"
                default="0"
                class="input-xlarge">
            <option value="0">COM_LICENSING_NO</option>
            <option value="1">COM_LICENSING_YES</option>
        </field>
        <field
                name="count"
                type="text"
                label="COM_LICENSING_SOFTWARE_HEAD_COUNT"
                description="COM_LICENSING_SOFTWARE_HEAD_COUNT_DESC"
                autocomplete="off"
                class="input-xlarge"
        />
        <field
                name="countAvalible"
                type="text"
                label="COM_LICENSING_SOFTWARE_HEAD_COUNT_AVALIBLE"
                description="COM_LICENSING_SOFTWARE_HEAD_COUNT_AVALIBLE_DESC"
                autocomplete="off"
                class="input-xlarge"
        />
        <field
                name="countReserv"
                type="text"
                label="COM_LICENSING_SOFTWARE_HEAD_COUNT_RESERV"
                description="COM_LICENSING_SOFTWARE_HEAD_COUNT_RESERV_DESC"
                autocomplete="off"
                class="input-xlarge"
        />
        <field
                name="about"
                type="editor"
                label="COM_LICENSING_SOFTWARE_HEAD_ABOUT"
                description="COM_LICENSING_SOFTWARE_HEAD_ABOUT_DESC"
                filter="raw"
                class="input-xlarge"
        />
        <field
                name="state"
                type="list"
                label="JSTATUS"
                description="JFIELD_PUBLISHED_DESC"
                class="inputbox"
                size="1"
                default="1"
        >
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
            <option value="2">JARCHIVED</option>
            <option value="-2">JTRASHED</option>
        </field>
    </fieldset>
</form>