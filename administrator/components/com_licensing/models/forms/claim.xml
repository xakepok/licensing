<?xml version="1.0" encoding="utf-8"?>
<form addrulepath="/administrator/components/com_licensing/models/rules">
    <fieldset name="names">
        <field
                name="id"
                type="hidden"
        />
        <field
                name="empl_guid"
                type="Employer"
                label="COM_LICENSING_CLAIMS_HEAD_EMPLOYER"
                description="COM_LICENSING_CLAIMS_HEAD_EMPLOYER"
                required="true"
                class="input-xlarge"
        />
        <field
                name="structure"
                type="Structure"
                label="COM_LICENSING_CLAIMS_HEAD_STRUCTURE"
                description="COM_LICENSING_CLAIMS_HEAD_STRUCTURE"
                required="true"
                autocomplete="off"
                class="input-xlarge"
        />
        <field
                name="email"
                type="email"
                label="COM_LICENSING_CLAIMS_HEAD_EMAIL"
                description="COM_LICENSING_CLAIMS_HEAD_EMAIL"
                required="true"
                autocomplete="off"
                class="input-xlarge"
        />
        <field
                name="phone"
                type="text"
                label="COM_LICENSING_CLAIMS_HEAD_PHONE"
                description="COM_LICENSING_CLAIMS_HEAD_PHONE"
                required="true"
                autocomplete="off"
                class="input-xlarge"
        />
        <field
                name="state"
                type="list"
                label="JSTATUS"
                description="JFIELD_PUBLISHED_DESC"
                class="inputbox"
                size="1"
                default="0"
        >
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
            <option value="2">JARCHIVED</option>
            <option value="-2">JTRASHED</option>
        </field>
        <field
                name="comment"
                type="text"
                label="COM_LICENSING_CLAIMS_HEAD_COMMENT"
                description="COM_LICENSING_CLAIMS_HEAD_COMMENT"
                class="input-xlarge"
        />
        <field
                name="empl_fio"
                type="hidden"
        />
        <field
                name="uid"
                type="hidden"
        />
    </fieldset>
</form>