<?xml version="1.0" encoding="utf-8"?>
<form addrulepath="/administrator/components/com_licensing/models/rules">
    <fieldset name="names">
        <field
                name="id"
                type="hidden"
        />
        <field
                name="softwareID"
                type="Software"
                label="COM_LICENSING_SOFTWARE_HEAD_NAME"
                description="COM_LICENSING_SOFTWARE_HEAD_NAME_DESC"
                required="true"
                class="input-xlarge"
        />
        <field
                name="type"
                type="Keytype"
                label="COM_LICENSING_KYETYPE_HEAD_TYPE"
                description="COM_LICENSING_KYETYPE_HEAD_TYPE_DESC"
                required="true"
                class="input-xlarge"
        />
        <field
                name="text"
                type="text"
                label="COM_LICENSING_KEY_HEAD_KEY"
                description="COM_LICENSING_KEY_HEAD_KEY_DESC"
                required="true"
                autocomplete="off"
                class="input-xlarge"
        />
        <field
                name="state"
                type="list"
                label="JSTATUS"
                description="JFIELD_PUBLISHED_DESC"
                class="inputbox"
                size="1"
                default="1"
        >
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
            <option value="2">JARCHIVED</option>
            <option value="-2">JTRASHED</option>
        </field>
    </fieldset>
</form>